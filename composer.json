{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "adldap2/adldap2-laravel": "^6.0",
        "aranyasen/hl7": "^1.7",
        "awobaz/compoships": "^1.1",
        "barryvdh/laravel-cors": "^0.11.2",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.0",
        "gumlet/php-image-resize": "^1.9",
        "guzzlehttp/guzzle": "^6.3",
        "intervention/image": "^2.4",
        "khanamiryan/qrcode-detector-decoder": "^1.0",
        "kylekatarnls/laravel-carbon-2": "^1.0.0",
        "laravel/framework": "5.7.*",
        "laravel/passport": "^7.2",
        "laravel/tinker": "^1.0",
        "nesbot/carbon": "2.22.0 as 1.34.0",
        "phpoffice/phpword": "^0.17.0",
        "picqer/php-barcode-generator": "^0.3.0",
        "rundiz/number": "^1.1",
        "santigarcor/laratrust": "5.2.*",
        "simplesoftwareio/simple-qrcode": "^2.0",
        "thecodingmachine/gotenberg-php-client": "^5.0",
        "tinybutstrong/opentbs": "^1.9",
        "tom-lingham/searchy": "^2.0",
        "uvinum/pdf-watermarker": "^1.0",
        "venturecraft/revisionable": "1.*"
    },
    "require-dev": {
        "ajthinking/tinx": "^2.2",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
